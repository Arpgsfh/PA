<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.d3ifcool.denver"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:largeHeap="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ProfilAnakActivity"
            android:label="Profil Anak"
            android:theme="@style/AppThemeAction">
        </activity>
        <activity
            android:name=".StimulasiActivity"
            android:label="Stimulasi"
            android:theme="@style/AppThemeAction">
        </activity>
        <activity
            android:name=".RiwayatActivity"
            android:label="Riwayat"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppThemeAction">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PilihUmurActivity" />
        <activity
            android:name=".TestActivity"
            android:label="Test"
            android:theme="@style/AppThemeAction">
        </activity>
        <activity android:name=".PenilaianActivity" />
        <activity android:name=".LoginActivity" />
    </application>

</manifest>